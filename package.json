{
  "name": "hot-pro",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://xyweizhi.github.com/github-hot/",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,less}": [
      "prettier --write",
      "git add ."
    ],
    "src/**/*.{js,jsx}": "eslint ./src --ext .js,.jsx"
  },
  "scripts": {
    "start": "webpack serve --config ./webpack/webpack.dev.config.js",
    "build": "webpack --config ./webpack/webpack.prod.config.js",
    "lint": "eslint ./src --ext .js,.jsx",
    "prettier": "prettier **/* --write",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "ejs-loader": "^0.5.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "webpack": "^5.36.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "@loadable/component": "^5.15.0",
    "@lsky/http-react": "^0.0.4",
    "@lsky/tools": "^1.0.0",
    "antd": "^4.16.12",
    "axios": "^0.21.1",
    "font-awesome": "^4.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-infinite-scroller": "^1.2.4",
    "react-router-dom": "^5.2.0"
  },
  "private": true
}